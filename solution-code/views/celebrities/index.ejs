<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% celebrities.forEach( (theCelebrity) => { %>
        <li>
          <%= theCelebrity.name %> - <%= theCelebrity.occupation %>
        </li>
      <% }); %>
    </ul>

    <a href = "/celebrities/new">
      Click here to add a new celebrity
    </a>
  </body>
</html>
